lock     
faceplayer     
msgbox BaseRocketHoennS2_Text_13, MSGBOX_DEFAULT
release     
end

BaseRocketHoennS2_Text_13:
.string "Lo que no se define, no se puede medir,\n"
.string "lo que no se mide, no se puede mejorar,\l"
.string "y todo lo que no se mejora está\l"
.string "condenado a la degradación.$"





BaseRocketHoennS2_15::
lock     
faceplayer     
